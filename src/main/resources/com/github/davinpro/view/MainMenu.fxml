<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="apMainMenu" prefHeight="400.0" prefWidth="600.0" stylesheets="@../style sheets/darkmode.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.davinpro.viewmodel.MainMenu">
   <children>
      <Label fx:id="lblTitle" alignment="CENTER" layoutX="161.0" layoutY="26.0" text="Snake Game" textAlignment="CENTER" textFill="#0c8618">
         <font>
            <Font name="System Bold" size="48.0" />
         </font>
      </Label>
      <Button fx:id="btnOptions" layoutX="229.0" layoutY="175.0" mnemonicParsing="false" onAction="#openOptions" prefHeight="50.0" prefWidth="142.0" text="Options" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnStart" layoutX="229.0" layoutY="251.0" mnemonicParsing="false" onAction="#startGame" prefHeight="50.0" prefWidth="142.0" text="Start">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
