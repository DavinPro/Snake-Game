<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" stylesheets="@/com/github/davinpro/style sheets/darkmode.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.davinpro.viewmodel.EndScreenController">
<Label text="GAME OVER" textFill="#0c8618">
  <font>
    <Font name="System Bold" size="48.0" />
  </font>
      <VBox.margin>
         <Insets bottom="10.0" />
      </VBox.margin>
</Label>
<Label fx:id="name" text="Player Name" textFill="#0c8618">
 <font>
   <Font size="24.0" />
 </font>
      <VBox.margin>
         <Insets bottom="20.0" top="20.0" />
      </VBox.margin>
</Label>
  <GridPane hgap="10.0" prefWidth="600.0" vgap="10.0">
    <columnConstraints>
      <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <Label text="Score:" textFill="#0c8618">
      <font>
        <Font size="24.0" />
      </font>
    </Label>
      <Label text="Time:" textFill="#0c8618" GridPane.rowIndex="1">
        <font>
          <Font size="24.0" />
        </font>
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
      </Label>
      <Label fx:id="score" text="0" textFill="#0c8618" GridPane.columnIndex="1">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="time" text="0m 0.0s" textFill="#0c8618" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <font>
          <Font size="24.0" />
        </font>
      </Label>
      <VBox.margin>
         <Insets right="40.0" top="20.0" />
      </VBox.margin>
  </GridPane>
<Button mnemonicParsing="false" onAction="#switchToMainMenu" text="Menu">
      <VBox.margin>
         <Insets top="120.0" />
      </VBox.margin>
   </Button>
</VBox>
